<mapper namespace="cn.keyshen.jjinfo.dao.AlbumMapper" >

    <insert id="addAlbum"  parameterType="Album">
           insert into album (albumName,coverPath,publishTime)  values (#{albumName},#{coverPath},#{publishTime})
    </insert>

</mapper>
